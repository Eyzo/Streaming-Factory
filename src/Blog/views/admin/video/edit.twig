{% extends '@Admin/layout.twig' %}

{% block title "Editer l'episode" %}


{% block body %}

<h1>Editer l'episode de {{ episode.slug }}:</h1>
 
<form action="{{ path(routePrefix ~'.edit',{id_episode:id_episode,id:id}) }}" method="POST">
	{% include viewPath ~ '/form.twig' %}
